{
  "name": "@reservando/frontend",
  "version": "1.0.0",
  "description": "Reservando frontend",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": " NODE_ENV=production node index.js",
    "start:dev": "NODE_ENV=development nodemon index.js",
    "build": "if-env NODE_ENV=production && npm run build:prod || npm run build:dev",
    "build:prod": "NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "build:dev": "NODE_ENV=development webpack --config build/webpack.config.dev.js",
    "build:analyze": "BUILD=analyze npm run build",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:fix": "npm run lint:prettier -- --write && npm run lint:eslint -- --fix",
    "lint:eslint": "eslint '**/*.{js,ts,vue}'",
    "lint:prettier": "prettier --check '**/*.{md,scss,json}'",
    "watch": "npm run build:dev -- --watch"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/koa": "^2.11.6",
    "@types/koa-helmet": "^6.0.2",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.14.20",
    "@types/webpack": "^4.41.25",
    "@vue/test-utils": "^1.1.3",
    "autoprefixer": "^9.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.1.1",
    "css-loader": "^1.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "fork-ts-checker-webpack-plugin": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "nodemon": "^1.19.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.1.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.32.7",
    "sass-loader": "^7.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^1.4.3",
    "ts-loader": "^5.4.5",
    "typescript": "^3.9.8",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": ">=0.18.1",
    "camelcase-keys": "^6.2.2",
    "core-js": "^3.6.5",
    "cuid": "2.1.4",
    "dd-trace": "^0.10.2",
    "envfile": "3.0.0",
    "file-loader": "2.0.0",
    "firebase": "^7.24.0",
    "if-env": "^1.0.4",
    "koa": "2.5.3",
    "koa-helmet": "4.0.0",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "query-string": "^6.14.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "8.3.2",
    "vue-observe-visibility": "^1.0.0",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2"
  },
  "nodemonConfig": {
    "delay": "2000",
    "ignore": [
      "dist/*"
    ]
  }
}
