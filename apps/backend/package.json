{
  "name": "@reservando/backend",
  "version": "1.0.0",
  "description": "Reservando api",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "start:dev": "NODE_ENV=development nodemon --inspect=0.0.0.0:9229 dist/index.js",
    "build": "tsc",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@types/koa": "^2.11.8",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^6.0.2",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "^8.0.4",
    "@types/node": "^14.14.27",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@reservando/commons": "^1.0.0",
    "axios": ">=0.21.1",
    "firebase-admin": "^9.5.0",
    "if-env": "^1.0.4",
    "koa": "2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "6.0.0"
  },
  "nodemonConfig": {
    "delay": "1000",
    "events": {
      "start": "kill-port 3005,9229",
      "restart": "kill-port 3005,9229"
    }
  }
}
